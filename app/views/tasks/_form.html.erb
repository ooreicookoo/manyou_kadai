<%= form_with(model:@task, local: true) do |form| %>
  <div class="field">
  <%= form.label :title %>
  <%= form.text_field :title %>
  </div>
  <div class="field">
  <%= form.label :content %>
  <%= form.text_field :content %>
  </div>
  <div class="field">
  <%= form.label :priority %>
  <%= form.select :priority, ["高","中","低"] %>
  </div>
  <div class="field">
  <%= form.label :status %>
  <%= form.select :status, ["未着手","着手","完了"]%>
  </div>
  <div class="field">
  <%= form.label :limit %>
  <%= form.date_select :limit %>
  </div>
  <%= form.submit %>
<% end %>
